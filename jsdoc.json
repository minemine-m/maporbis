{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["./dist-docs-temp"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "categories": "Map, Layer, Geometry, Basic Types, MapTool, UI, Control, Utils, TileSystem, Projection",
    "default": {
      "useLongnameInNav": true,
      "layoutFile": "./docs/layout.tmpl",
      "staticFiles": {
        "include": [
            "./docs/static"
        ]
      }
    }
  },
  "opts": {
    "encoding": "utf8",
    "destination": "./docs/jsdoc/",
    "recurse": true,
    "template": "./docs/template"
  }
}
